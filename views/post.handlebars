{{#each posts as |post|}}
<div class="yourPosts">
    <div class="postTitle">
        {{this.title}}
    </div>
    <div class="postContent">
        {{this.post}}
    </div>
</div>

{{/each}}

<script>
    document.querySelector("#dashBtn").addEventListener("click", function (event) {
        event.preventDefault();
        document.location.replace("/dashboard");
<>
</script>